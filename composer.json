{
  "name": "goddard_profile",
  "description": "Goddard Installation Profile",
  "type": "drupal-profile",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "~1.0",
        "drupal/admin_toolbar": "^1.22",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/sitemap": "^1.3",
        "drupal/address": "^1.3"
        "drupal/calendar": "1.x-dev",
        "drupal/pathauto": "^1.0",
        "drupal/metatag": "^1.4"        
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/modules/contrib/{$name}/": ["type:drupal-module"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ]
}